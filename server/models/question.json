{
  "name": "Question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "order": {
      "type": "number",
      "required": true
    },
    "form": {
      "type": "string",
      "required": true,
      "length": 1
    },
    "archivedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "word": {
      "type": "belongsTo",
      "model": "Word",
      "foreignKey": "linkId"
    },
    "conjugation": {
      "type": "belongsTo",
      "model": "Conjugation",
      "foreignKey": "linkId"
    },
    "answers": {
      "type": "hasMany",
      "model": "Answer",
      "foreignKey": ""
    },
    "test": {
      "type": "belongsTo",
      "model": "Test",
      "foreignKey": ""
    },
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
